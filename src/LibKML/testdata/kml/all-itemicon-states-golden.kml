<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>ItemIcon examples</name>
    <open>1</open>
    <Style id="open_folder">
      <ListStyle>
        <ItemIcon>
          <state>open</state>
          <href>icons/openfolder.png</href>
        </ItemIcon>
      </ListStyle>
    </Style>
    <Style id="closed_folder">
      <ListStyle>
