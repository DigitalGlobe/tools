<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <Style id="i27">
      <IconStyle>
        <Icon>
          <href>weather-pngs.kmz/icons_small/27.png</href>
        </Icon>
      </IconStyle>
      <LabelStyle>
        <color>ff7ccfff</color>
      </LabelStyle>
      <BalloonStyle>
        <text>
          <![CDATA[
          <table>
            <tr>
              <td colspan="5">
                <font size="+1">Currently in $[location]</font>
              </td>
            </tr>
            <br/>
            <tr>
              <td>
                <br/>
                <img src="weather-jpgs.kmz/icons_jpg/$[00_Icon].jpg"/>
                <br/>
                <b>$[00_Phrase]</b>
              </td>
              <td valign="center">
                <b>
                  <font size="7">$[name]</font>
                </b>
                <br/>
                <b>Feels like $[00_Feels_Like]</b>
              </td>
              <td valign="center">Humidity: $[00_Humidity]<br/>Wind: $[00_Wind_Direction]
                $[00_Wind_Speed]<br/>
              </td>
            </tr>
          </table>
          ]]>
        </text>
      </BalloonStyle>
    </Style>
  </Document>
</kml>
