<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document id="doc">
    <!-- Relative URL -->
    <styleUrl>allstyles.kml#allstyles</styleUrl>
    <Placemark id="placemark">
      <!-- Relative URL to a StyleMap -->
      <styleUrl>shared-stylemap.kml#msn_ylw-pushpin</styleUrl>
    </Placemark>
    <Placemark id="placemark-noid">
      <!-- Relative URL to a file that exists with with no such id -->
      <styleUrl>shared-stylemap.kml#no-such-id</styleUrl>
    </Placemark>
    <Placemark id="placemark-nofile">
      <!-- Relative URL to a non-existent file -->
      <styleUrl>no-such-file.kml#no-such-id</styleUrl>
    </Placemark>
    <Placemark id="placemark-inline">
      <!-- No styleUrl -->
      <Style>
        <LineStyle>
          <width>3.3</width>
        </LineStyle>
      </Style>
    </Placemark>
    <Placemark id="placemark-style-both">
      <!-- Both a styleUrl and inline Style -->
      <styleUrl>shared-stylemap.kml#sn_ylw-pushpin</styleUrl>
      <Style>
        <PolyStyle>
          <outline>true</outline>
        </PolyStyle>
      </Style>
    </Placemark>
    <Placemark id="placemark-stylemap-both">
      <!-- Both a styleUrl and inline Style -->
      <styleUrl>shared-stylemap.kml#msn_ylw-pushpin</styleUrl>
      <StyleMap>
      </StyleMap>
    </Placemark>
    <GroundOverlay id="groundoverlay">
      <!-- Absolute URL -->
      <styleUrl>http://host.com/style/iconstyle.kml#plane-icon</styleUrl>
    </GroundOverlay>
    <ScreenOverlay id="screenoverlay">
      <!-- Relative URL to a Style -->
      <styleUrl>shared-stylemap.kml#sn_ylw-pushpin</styleUrl>
    </ScreenOverlay>
    <PhotoOverlay id="photooverlay">
      <!-- Absolute URL to a StyleMap -->
      <styleUrl>http://ignored.com/style/shared-stylemap.kml#msn_ylw-pushpin</styleUrl>
    </PhotoOverlay>
    <NetworkLink id="networklink">
      <!-- no styleUrl and no StyleSelector -->
    </NetworkLink>
    <Folder id="folder">
      <styleUrl>iconstyle.kml#train-icon</styleUrl>
      <Style>
        <ListStyle>
          <listItemType>radioFolder</listItemType>
        </ListStyle>
      </Style>
    </Folder>
  </Document>
</kml>
